#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    
    int T;
    if(!(cin >> T)) return 0;
    while (T--) {
        long long n;
        cin >> n;
        vector<long long> a(n);
        for (long long i = 0; i < n; ++i) cin >> a[i];

        long long ans = 0;
        if (n % 2 == 1) {
            // only even indices (0-based) matter when n is odd
            for (long long i = 0; i < n; i += 2) ans ^= a[i];
        }
        cout << ans << '\n';
    }
    return 0;
}
