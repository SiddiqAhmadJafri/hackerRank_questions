#include <stdio.h>

void solve() {
    int m, n;
    scanf("%d", &m);
    scanf("%d", &n);
    int cost[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &cost[i]);
    }

    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            if (cost[i] + cost[j] == m) {
                printf("%d %d\n", i + 1, j + 1);
                return; // Found the solution, exit
            }
        }
    }
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        solve();
    }
    return 0;
}
