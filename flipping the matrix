#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int q; 
    cin >> q;
    while (q--) {
        int n;
        cin >> n;
        int size = 2 * n;
        vector<vector<int>> a(size, vector<int>(size));

        for (int i = 0; i < size; i++)
            for (int j = 0; j < size; j++)
                cin >> a[i][j];

        long long ans = 0;
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                ans += max({
                    a[i][j],
                    a[i][size - 1 - j],
                    a[size - 1 - i][j],
                    a[size - 1 - i][size - 1 - j]
                });
            }
        }
        cout << ans << "\n";
    }

    return 0;
}
